{
    "name": "theme-neptune",
    "toolkit": "classic",
    "alternateName": ["ext-theme-neptune"],
    "type": "theme",
    "creator": "Sencha",
    "summary": "Ext JS Neptune Theme",
    "detailedDescription": "The Neptune Theme provides a clean, modern style for Ext JS",
    "version": "6.0.0.415",
    "compatVersion": "5.0.0",
    "format": "1",
    "slicer": {
        "js": [
            {
                "path": "${package.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    },
    "output": "${workspace.subpkg.prefix}/${package.name}",
    "local": true,
    "requires": [],
    "extend": "theme-neutral"
}
