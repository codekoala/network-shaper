{
    "name": "theme-crisp",
    "toolkit": "classic",
    "alternateName": ["ext-theme-crisp"],
    "type": "theme",
    "creator": "Sencha",
    "summary": "Ext JS Crisp Theme",
    "detailedDescription": "The Crisp Theme builds upon Neptune, adding a light, clean look and feel.",
    "version": "6.0.0.415",
    "compatVersion": "5.0.0",
    "format": "1",
    "slicer": {
        "js": [
            {
                "path": "${package.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    },
    "output": "${workspace.subpkg.prefix}/${package.name}",
    "requires": [],
    "extend": "theme-neptune",
    "local": true
}
